<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#ff5bb430" />
            <corners android:radius="5.0dip" />
            <stroke android:width="1.0dip" android:color="#ff5bb430" />
            <padding android:left="2.0dip" android:top="2.0dip" android:right="2.0dip" android:bottom="2.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#ff777777" />
            <stroke android:width="1.0dip" android:color="#ff777777" />
            <corners android:radius="5.0dip" />
        </shape>
